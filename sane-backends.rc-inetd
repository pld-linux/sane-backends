# rc-inetd configuration for saned --- remote scanning daemon
#
# /etc/sysconfig/rc-inetd

SERVICE_NAME=sane
SOCK_TYPE=stream
PROTOCOL=tcp
PORT=6566
FLAGS=nowait
USER=saned
SERVER=tcpd
DAEMON=/usr/sbin/saned

# This variables are optional:

DAEMONARGS=
MAX_CONECTIONS=
GROUP=saned
NICE=
FAMILY=ipv4
INTERFACE=
CHROOT=
RPCNAME=
RPCVERSION=
INITGROUPS=
BANNER=
ECHO=
FILTER=
ENV=
